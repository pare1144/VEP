<Window x:Class="VEP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VEP"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="191*"/>
            <ColumnDefinition Width="68*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="StartButton" Content="Start" HorizontalAlignment="Left" Height="21" Margin="29,22,0,0" VerticalAlignment="Top" Width="73" Click="StartButton_Click"/>
        <Button x:Name="EditButton" Content="Edit" HorizontalAlignment="Left" Height="21" Margin="185,22,0,0" VerticalAlignment="Top" Width="73
                " Click="EditButton_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="251" Margin="29,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="479" Grid.ColumnSpan="2"/>
        <Button x:Name="StopButton" Content="Stop" HorizontalAlignment="Left" Height="21" Margin="107,22,0,0" VerticalAlignment="Top" Width="73" Click="StopButton_Click"/>

    </Grid>
</Window>
